I"Ç(function() {
  $(document).ready(function() {
    return $(".add_to_cart").on("click", function() {
      var $t, params, qty;
      $t = $(this);
      qty = $t.parents(".product").find('option').val();
      qty = parseInt(qty);
      if ((qty == null) || qty <= 0) {
        return false;
      } else {
        params = $t.data("params");
        params.quantity = qty;
        return $t.data("params", params);
      }
    });
  });

}).call(this);
